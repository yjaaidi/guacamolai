{
  "name": "@guacamolai/guacamolai",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "nx": {
    "projectType": "application",
    "tags": [
      "type:app"
    ],
    "targets": {
      "e2e": {
        "dependsOn": [
          "build"
        ]
      },
      "prepare-release": {
        "command": "zip -r guacamolai.zip guacamolai",
        "options": {
          "cwd": "{projectRoot}/dist"
        }
      }
    }
  }
}
